<module>
  <inherits name='org.realityforge.braincheck.BrainCheck'/>

  <source path='' includes='*.java'/>

  <!--
    These properties are all defined in galdr.GaldrConfig
    See that class for a better understanding of what these properties do.
  -->

  <define-property name='galdr.environment' values='development,production'/>
  <set-property name='galdr.environment' value='production'/>

  <define-property name='galdr.enable_names' values='true,false'/>
  <set-property name='galdr.enable_names' value='false'/>

  <define-property name='galdr.copy_arrays_passed_to_constructors' values='true,false'/>
  <set-property name='galdr.copy_arrays_passed_to_constructors' value='false'/>

  <define-property name='galdr.debug_to_string' values='true,false'/>
  <set-property name='galdr.debug_to_string' value='false'/>

  <define-property name='galdr.enable_error_handlers' values='true,false'/>
  <set-property name='galdr.enable_error_handlers' value='true'/>

  <define-property name='galdr.check_invariants' values='true,false'/>
  <set-property name='galdr.check_invariants' value='false'/>

  <define-property name='galdr.check_api_invariants' values='true,false'/>
  <set-property name='galdr.check_api_invariants' value='false'/>

  <define-property name='galdr.logger' values='console_js,jul,proxy,none'/>
  <set-property name='galdr.logger' value='console_js'/>
</module>
