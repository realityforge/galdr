<module>
  <inherits name='galdr.Galdr'/>

  <source path='ignored'/>

  <set-property name='galdr.environment' value='development'/>
  <set-property name='galdr.enable_names' value='true'/>
  <set-property name='galdr.enable_spies' value='true'/>
  <set-property name='galdr.copy_arrays_passed_to_constructors' value='true'/>
  <set-property name='galdr.debug_to_string' value='true'/>
  <set-property name='galdr.enforce_unmodifiable_collections' value='true'/>
  <set-property name='galdr.check_invariants' value='true'/>
  <set-property name='galdr.check_api_invariants' value='true'/>
</module>
